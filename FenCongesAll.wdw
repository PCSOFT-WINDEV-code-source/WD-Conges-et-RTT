      ddate est une date
      ddate-=15
      // Initialise le planning
      clPlanning:InitialisePeriode(ddate)
      
      // Affichage des congés actuels
      AfficheCongésAll()
      
      // Affichage
      clPlanning:TracePlanning()
      
      // Bornes de l'ascenseur
      Ascenseur1..BorneMin=clPlanning:DateDebut
      Ascenseur1..BorneMax=Ascenseur1..BorneMin+365-clPlanning:NombreColonnes-Val(EntierVersDate(Ascenseur1..BorneMin)[[7 A 8]])
      Ascenseur1..AvanceRapide=30
      Ascenseur1 =clPlanning:DateDebut
