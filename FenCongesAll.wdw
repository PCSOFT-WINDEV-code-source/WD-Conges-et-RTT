  procedures :
   -
     name : AfficheCongésAll
     procedure_id : 4497017591567217469
     type_code : 14
     code : |1+
      Procédure AfficheCongésAll()
      // Affichage des demandes de congés de tout le monde
      
      bMidiD, bMidiF sont des booléens
      nCouleur est un entier
      i est un entier
      
      // on efface tout
      clPlanning:PlageSupprimeTout()
      
      // affichage des collaborateurs
      HLitPremier(GROUPE,Ordre)
      TANTQUE PAS HEnDehors()
      	
      	// Collaborateur du groupe
      	HFiltre(COLLABORATEUR,Numéro,Caract(0),Caract(255),"COLLABORATEUR.IDGROUPE="+GROUPE.IDGROUPE)
      	
      	// parcours des collaborateurs du groupe
      	HLitPremier(COLLABORATEUR,Numéro)
      	TANTQUE PAS HEnDehors()
      		
      		// Ajout d'une ligne au planning par collaborateur
      		clPlanning:AjouteItem(COLLABORATEUR.Pseudo,1, chaineconstruit(<§$0000§>, COLLABORATEUR.Pseudo))
      		i++
      		
      		// Demande de congés de ce collaborateur
      		HFiltre(DEMANDE, InitialesDateFin, [COLLABORATEUR.Pseudo+Caract(0),entierversdate(clPlanning:DateDebut)], [COLLABORATEUR.Pseudo+Caract(0),Caract(255)])
      		// Parcours des demandes
      		HLitPremier(DEMANDE,InitialesDateFin)
      		TANTQUE PAS HEnDehors()
      							
      			SI DEMANDE.HeureDébut~=CST_MIDI ALORS 
      				bMidiD = Vrai
      			SINON
      				bMidiD = Faux
      			FIN
      		
      			SI DEMANDE.HeureFin~=CST_MIDI ALORS 
      				bMidiF = Vrai
      			SINON
      				bMidiF = Faux
      			FIN
      		
      			nCouleur = iBlanc
      			SI DEMANDE.ValidéDirection ALORS
      				nCouleur= RVB(238,232,115)
      			FIN
      			SI DEMANDE.Comptabilisé1 ALORS
      				nCouleur= RVB(158, 252, 114)
      			FIN
      		
      			// Ajout des congés
      			AjoutePlage(i, DEMANDE.DateDébut, bMidiD, DEMANDE.Datefin, bMidiF, 0, DEMANDE.Observation, nCouleur)
      		
      			HLitSuivant(DEMANDE,InitialesDateFin)
      		FIN
      	
      	HDésactiveFiltre(DEMANDE)
      	HLitSuivant(COLLABORATEUR,Numéro)
      	fin
      
      	// Ajout d'une ligne vide
      	clPlanning:AjouteItem("",1,"")
      	i++
      
      	HLitSuivant(GROUPE,ordre)
      fin
      
      HDésactiveFiltre(COLLABORATEUR)
